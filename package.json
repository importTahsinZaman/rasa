{
  "name": "rasa-ui-customizer",
  "version": "1.0.0",
  "description": "Chrome extension that allows users to customize any website's UI through natural language commands",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "build": "wxt build && npm run copy-icons",
    "copy-icons": "cp -r public/icon .output/chrome-mv3/",
    "zip": "wxt zip",
    "postinstall": "wxt prepare",
    "generate-icons": "node scripts/generate-icons.cjs"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@types/chrome": "^0.0.279",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@wxt-dev/module-react": "^1.1.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "wxt": "^0.19.16"
  }
}
